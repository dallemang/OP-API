webinar:BindBySelect_1
      rdf:type sml:BindBySelect ;
      rdfs:label "Bind by select 1"^^xsd:string ;
      sm:next webinar:target_1 ;
      sm:nodeX 522 ;
      sm:nodeY 263 ;
      sml:selectQuery
              [ rdf:type sp:Select ;
                sp:where ([ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type smf:trace ;
                                      sp:arg1 [ sp:varName "uri"^^xsd:string
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "trace"^^xsd:string
                                    ]
                          ])
              ] .

webinar:CatchExceptions_1
      rdf:type sml:CatchExceptions ;
      rdfs:label "Catch exceptions 1"^^xsd:string ;
      sm:body webinar:compound-pharmacology-pages_1 ;
      sm:nodeX 342 ;
      sm:nodeY 147 ;
      sm:outputVariable "error"^^xsd:string .

webinar:CatchExceptions_2
      rdf:type sml:CatchExceptions ;
      rdfs:label "Catch exceptions 2"^^xsd:string ;
      sm:body webinar:BindBySelect_1 ;
      sm:nodeX 318 ;
      sm:nodeY 273 ;
      sm:outputVariable "error"^^xsd:string .

webinar:ConcatenateText_1
      rdf:type sml:ConcatenateText ;
      rdfs:label "Concatenate text 1"^^xsd:string ;
      sm:next webinar:HelloWorld_Return ;
      sm:nodeX 108 ;
      sm:nodeY 403 ;
      sm:outputVariable "text" ;
      sml:selectQuery
              [ rdf:type sp:Select ;
                sp:distinct "true"^^xsd:boolean ;
                sp:resultVariables ([ sp:varName "text"^^xsd:string
                          ]) ;
                sp:where ([ sp:object
                                    [ sp:varName "cw_uri"^^xsd:string
                                    ] ;
                            sp:predicate <http://www.w3.org/2004/02/skos/core#exactMatch> ;
                            sp:subject
                                    [ sp:varName "ops_item"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "uniprot_uri"^^xsd:string
                                    ] ;
                            sp:predicate <http://www.w3.org/2004/02/skos/core#exactMatch> ;
                            sp:subject
                                    [ sp:varName "ops_item"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "equiv_target"^^xsd:string
                                    ] ;
                            sp:predicate <http://www.w3.org/2004/02/skos/core#exactMatch> ;
                            sp:subject
                                    [ sp:varName "ops_item"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "db_uri"^^xsd:string
                                    ] ;
                            sp:predicate <http://www.w3.org/2004/02/skos/core#exactMatch> ;
                            sp:subject
                                    [ sp:varName "ops_item"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "target_name"^^xsd:string
                                    ] ;
                            sp:predicate <http://www.w3.org/2004/02/skos/core#prefLabel> ;
                            sp:subject
                                    [ sp:varName "cw_uri"^^xsd:string
                                    ]
                          ] [ sp:object <http://www.conceptwiki.org> ;
                            sp:predicate void:inDataset ;
                            sp:subject
                                    [ sp:varName "cw_uri"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "existence"^^xsd:string
                                    ] ;
                            sp:predicate uniprot:existence ;
                            sp:subject
                                    [ sp:varName "uniprot_uri"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "organism"^^xsd:string
                                    ] ;
                            sp:predicate uniprot:organism ;
                            sp:subject
                                    [ sp:varName "uniprot_uri"^^xsd:string
                                    ]
                          ] [ sp:object <http://purl.uniprot.org> ;
                            sp:predicate void:inDataset ;
                            sp:subject
                                    [ sp:varName "uniprot_uri"^^xsd:string
                                    ]
                          ] [ sp:object
                                    [ sp:varName "description"^^xsd:string
                                    ] ;
                            sp:predicate onto:hasDescription ;
                            sp:subject
                                    [ sp:varName "equiv_target"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type smf:buildString ;
                                      sp:arg1 "<tr><td>{?target_name}</td><td>{?1}</td><td>{?description}</td></tr>" ;
                                      sp:arg2 [ rdf:type smf:htmlString ;
                                                sp:arg1 [ rdf:type xsd:string ;
                                                          sp:arg1 [ sp:varName "organism"^^xsd:string
                                                                  ]
                                                        ]
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "text"^^xsd:string
                                    ]
                          ])
              ] .

webinar:HelloWorld
      rdf:type sm:Function ;
      rdfs:comment "Hello World application based on webinar from Paul Groth"^^xsd:string ;
      rdfs:subClassOf sm:Functions ;
      spin:constraint
              [ rdf:type spl:Argument ;
                spl:predicate sp:arg1 ;
                spl:valueType xsd:string ;
                sm:next webinar:SearchByName ;
                sm:nodeX 496 ;
                sm:nodeY 15
              ] ;
      sm:returnModule webinar:HelloWorld_Return .

webinar:HelloWorld_Return
      rdf:type sml:ReturnText ;
      sm:nodeX 473 ;
      sm:nodeY 403 ;
      sml:mimeType "text/html"^^xsd:string ;
      sml:text "<html><head/><body><h2>Targets for {?arg1}</h2><table border=1><tr><th>Target</th><th>Organism</th><th>Description</th></tr>{?text}</table></body></html>"^^xsd:string .

webinar:IterateOverSelect_1
      rdf:type sml:IterateOverSelect ;
      rdfs:label "Iterate over select 1"^^xsd:string ;
      sm:body webinar:CatchExceptions_1 ;
      sm:next webinar:IterateOverSelect_2 ;
      sm:nodeX 105 ;
      sm:nodeY 137 ;
      sml:selectQuery
              [ rdf:type sp:Select ;
                sp:distinct "true"^^xsd:boolean ;
                sp:resultVariables ([ sp:varName "uri"^^xsd:string
                          ]) ;
                sp:where ([ sp:object
                                    [ sp:varName "u"^^xsd:string
                                    ] ;
                            sp:predicate ops_api:result ;
                            sp:subject
                                    [ sp:varName "primaryTopic"^^xsd:string
                                    ]
                          ] [ rdf:type sp:Bind ;
                            sp:expression
                                    [ rdf:type xsd:string ;
                                      sp:arg1 [ sp:varName "u"^^xsd:string
                                              ]
                                    ] ;
                            sp:variable
                                    [ sp:varName "uri"^^xsd:string
                                    ]
                          ])
              ] .

webinar:IterateOverSelect_2
      rdf:type sml:IterateOverSelect ;
      rdfs:label "Iterate over select 2"^^xsd:string ;
      sm:body webinar:CatchExceptions_2 ;
      sm:next webinar:ConcatenateText_1 ;
      sm:nodeX 103 ;
      sm:nodeY 255 ;
      sml:selectQuery
              [ rdf:type sp:Select ;
                sp:resultVariables ([ sp:varName "uri"^^xsd:string
                          ]) ;
                sp:where ([ sp:object
                                    [ sp:varName "uri"^^xsd:string
                                    ] ;
                            sp:predicate onto:hasTarget ;
                            sp:subject
                                    [ sp:varName "s"^^xsd:string
                                    ]
                          ])
              ] .

